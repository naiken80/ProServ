{
  "name": "proserv",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "packageManager": "pnpm@10.19.0",
  "scripts": {
    "nx": "nx",
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "build": "nx run-many -t build",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "typecheck": "nx run-many -t typecheck --with-deps --skip-nx-cache",
    "security:audit": "pnpm audit --prod",
    "security:audit:full": "pnpm audit --audit-level=moderate",
    "prepare": "husky install",
    "db:generate": "pnpm exec prisma generate",
    "db:migrate": "pnpm exec prisma migrate dev",
    "db:seed": "pnpm exec prisma db seed",
    "db:studio": "pnpm exec prisma studio",
    "dev": "pnpm nx run-many -t serve --projects api,frontend --parallel",
    "dev:api": "pnpm nx serve api",
    "dev:frontend": "pnpm nx serve frontend",
    "stack:up": "docker compose up -d",
    "stack:down": "docker compose down"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@next/eslint-plugin-next": "^15.2.4",
    "@nx/devkit": "21.6.6",
    "@nx/eslint": "21.6.6",
    "@nx/eslint-plugin": "21.6.6",
    "@nx/jest": "21.6.6",
    "@nx/js": "21.6.6",
    "@nx/nest": "^21.6.6",
    "@nx/next": "21.6.6",
    "@nx/node": "21.6.6",
    "@nx/playwright": "21.6.6",
    "@nx/react": "21.6.6",
    "@nx/workspace": "21.6.6",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.0",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.12.0",
    "@typescript-eslint/parser": "^8.12.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jest": "^30.0.2",
    "jest-environment-jsdom": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^16.2.5",
    "nx": "21.6.6",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^6.18.0",
    "tailwindcss": "^3.4.13",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,css,scss,html,yml,yaml}": [
      "pnpm exec prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "pnpm exec eslint --max-warnings=0"
    ]
  },
  "engines": {
    "node": ">=20.18.0",
    "pnpm": ">=9.0.0"
  },
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@hookform/resolvers": "5.2.2",
    "@nestjs/common": "^11.1.7",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.7",
    "@nestjs/platform-fastify": "^11.1.7",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "^6.18.0",
    "@radix-ui/react-slot": "1.1.0",
    "@tanstack/react-query": "5.90.5",
    "@tanstack/react-query-devtools": "5.90.2",
    "axios": "^1.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "lucide-react": "0.546.0",
    "nestjs-pino": "^4.4.1",
    "next": "~15.2.4",
    "next-pwa": "5.6.0",
    "next-themes": "0.4.6",
    "pino": "^9.4.0",
    "pino-pretty": "^13.1.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "7.65.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tailwind-merge": "2.5.3",
    "tslib": "^2.8.1",
    "workbox-window": "7.3.0",
    "zod": "^3.23.8",
    "zustand": "5.0.8"
  },
  "prisma": {
    "seed": "pnpm exec ts-node --compiler-options '{\"module\":\"commonjs\"}' prisma/seed.ts"
  }
}
